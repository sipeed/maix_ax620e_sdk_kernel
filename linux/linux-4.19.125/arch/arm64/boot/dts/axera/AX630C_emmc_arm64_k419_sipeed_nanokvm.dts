	};

	jd9853: jd9853@1{
		compatible = "jadard,jd9853";
		reg = <1>;
		status = "okay";
		spi-max-frequency = <80000000>;
		spi-cpol;
		spi-cpha;
		rotate = <0>;
		fps = <90>;
		rgb;
		buswidth = <8>;
		dc-gpios = <&ax_gpio1 11 1>;
		reset-gpios = <&ax_gpio1 9 1>;
		te-gpios = <&ax_gpio2 13 1>;
		debug = <0x1>;
	};
};

