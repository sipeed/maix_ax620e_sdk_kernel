&spi2 {
	/* /delete-property/ dma-names; */
	/* /delete-property/ dmas; */
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart3_txd_spi_m2_mosi_pins>,
				<&i2c1_scl_spi_m2_sclk_pins>,
