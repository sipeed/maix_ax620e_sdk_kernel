&spi0 {
	status = "disabled";
	spidev_user@0 {
		status = "okay";
		compatible = "axera,spidev";
		reg = <0>;
		spi-max-frequency = <26000000>;
	};
};

&spi1 {
	/* /delete-property/ dma-names; */
	/* /delete-property/ dmas; */
	/delete-property/ cs-gpios;
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&vi_d0_spi_m1_mosi_pins>;

	spidev_user@0 {
		status = "okay";
		compatible = "axera,spidev";
