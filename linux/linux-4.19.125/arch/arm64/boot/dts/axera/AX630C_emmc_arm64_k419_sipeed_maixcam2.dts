	vfb0: vfb@0 {
		compatible = "axera,vfb";
		id = <0>;
		width = <480>;
		height = <640>;
		bpp = <32>;
		buf-num = <2>;
		status = "okay";
	};

	vfb1: vfb@1 {
		compatible = "axera,vfb";
		id = <1>;
		width = <1920>;
		height = <1080>;
		bpp = <32>;
		buf-num = <2>;
		status = "disable";
	};

	vfb2: vfb@2 {
		compatible = "axera,vfb";
		id = <2>;
		width = <64>;
		height = <64>;
		bpp = <32>;
		buf-num = <1>;
		cursor;
		status = "okay";
	};

	axera_logctl {
		compatible = "axera,logctl";
		config = "/etc/ax_syslog.conf";
		logstate = /bits/ 8 <1 1>;
		loglevel = /bits/ 8 <4 4>;
	};

	extcon_usb: extcon_usb {
		compatible = "linux,extcon-usb-gpio";
		id-gpio = <&ax_gpio1 13 0>; /* GPIO1_A13 */
		status = "okay";
	};
	leds {
		status = "okay";
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&vi_d6_gpio0_a6_pins>;
        led-heartbeat {
            label = "sys-heartbeat";
            gpios = <&ax_gpio0 6 0>;
            linux,default-trigger = "activity";
            default-state = "activity";
			status = "okay";
        };
	};

	rotary@0 {
		status = "okay";
		compatible = "rotary-encoder";
		pinctrl-names = "default";
		pinctrl-0 = <&rgmii_rxd2_gpio1_a16_pins>,
					<&rgmii_rxd3_gpio1_a17_pins>;
		gpios = <&ax_gpio1 16 1>, <&ax_gpio1 17 0>;
		linux,axis = <0>; /* REL_X */
		rotary-encoder,encoding = "gray";
		rotary-encoder,relative-axis;
		rotary-encoder,steps-per-period = <4>;
	};

	gpio_keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&gpio3_a2_pins>;
		// autorepeat;
		wakeup-source;
		status = "okay";

		key-user {
			label = "User Key";
			linux,code = <0x1af>;
			debounce-interval = <10>;
			gpios = <&ax_gpio3 2 1>;
		};
	};
	aic_bsp {
		compatible = "aicsemi,aic_bsp";
		reset-gpio = <&ax_gpio1 15 0>; // GPIO1_A15
		pinctrl-names = "default";
		pinctrl-gpios = <&rgmii_rxclk_gpio1_a15_pins>;
		status = "okay";
	};
};

&pwm0 {
	status = "okay";
};

&pwm1 {
	status = "okay";
};

&pwm2 {
	status = "okay";
};

&deb_gpio_lp {
	lp-status = <11 1>; /* use db_gpio11 as wakeup source */
	status = "okay";
};

&bwlimiter {
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";

	rtc: bm8563@51 {
		compatible = "nxp,pcf8563";  // BM8563 通常与 PCF8563 驱动兼容
		reg = <0x51>;               // I2C 地址（0x51）
		status = "okay";
	};

	cw2015@62 {
			compatible = "cellwise,cw2015";
			reg = <0x62>;
			cellwise,battery-profile = /bits/ 8 <
					0x15 0x39 0x5E 0x59 0x56 0x55 0x52 0x52
					0x51 0x54 0x51 0x47 0x3E 0x34 0x2B 0x26
					0x1E 0x16 0x13 0x1F 0x29 0x41 0x4F 0x4E
					0x71 0x9E 0x0A 0x3E 0x25 0x45 0x58 0x5F
					0x5A 0x62 0x68 0x6E 0x4A 0x1D 0x83 0x2E
					0x09 0x57 0x14 0x41 0x77 0x98 0xAC 0x3C
					0x4E 0x7B 0x99 0xAF 0x80 0xFF 0xFF 0xCB
					0x2F 0x00 0x64 0xA5 0xB5 0xC1 0x46 0xAE
					>;
			cellwise,monitor-interval-ms = <5000>;
			// monitored-battery = <&battery>;
			// power-supplies = <&charger>;
			status = "okay";
	};

	hynitron: hynitron@6a {
		compatible = "hynitron,hyn_ts", "hyn,7xx";
		reg = <0x6a>;
		irq-gpio = <&ax_gpio1 12 0>;
		reset-gpio = <&ax_gpio1 23 0>;
		max-touch-number = <5>;
		display-coords = <0 0 480 640>;
		pos-swap = <0>;
		posx-reverse = <1>;
		posy-reverse = <1>;
		status = "okay";
		wakeup-source;
	};

/*
	lsm6ds3@6b {
		compatible = "st,lsm6dsox";
		reg = <0x6b>;
		status = "okay";
	};
*/
};

&i2c6 {
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;
	/delete-property/ pinctrl-1;
	/delete-property/ scl-gpio;
	/delete-property/ sda-gpio;
	status = "okay";
};

&i2c7 {
	status = "okay";
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;
	/delete-property/ pinctrl-1;
	/delete-property/ scl-gpio;
	/delete-property/ sda-gpio;
	#address-cells = <0x1>;
	#size-cells = <0x0>;
};

&i2c_slv0 {
	status = "okay";
	#address-cells = <0x1>;
	#size-cells = <0x0>;
	i2c_slave_eeprom: i2c_slave_eeprom@55 {
		compatible = "axera,slave-24c02";
		reg = <0x40000055>;
		status = "okay";
	};
};

