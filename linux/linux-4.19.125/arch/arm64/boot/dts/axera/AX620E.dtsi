		};
		i2c5: i2c@4855000 {
			compatible = "snps,designware-i2c";
			reg = <0x0 0x4855000 0x0 0x400>;
			interrupts = <GIC_SPI 140 IRQ_TYPE_LEVEL_HIGH>;
			scl-gpio = <&ax_gpio0 10 0>;
			sda-gpio = <&ax_gpio0 4 0>;
