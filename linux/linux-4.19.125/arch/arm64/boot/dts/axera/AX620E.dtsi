			status = "disabled";

			usb2: dwc3@0x8000000 {
				compatible = "snps,dwc3";
				reg = <0x0 0x8000000 0x0 0x100000>;
				interrupts = <GIC_SPI 76 IRQ_TYPE_LEVEL_HIGH>;
				snps,dis-u2-freeclk-exists-quirk;
				dr_mode = "peirpheral";
				phy_type = "utmi";
				maximum-speed = "high-speed";
				usb2-only-mode;
				usb-role-switch;
			};
		};

		drm0: drm@0 {
