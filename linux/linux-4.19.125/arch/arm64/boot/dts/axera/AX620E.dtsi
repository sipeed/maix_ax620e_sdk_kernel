			reg-names = "reg_gzipd";
			interrupts = <GIC_SPI 81 IRQ_TYPE_LEVEL_HIGH>;
			status = "okay";

			clocks = <&flash_clk AX620X_CLK_GZIPD_EB>,
					 <&flash_clk AX620X_CLK_GZIPD_CORE_EB>;
