/dts-v1/;
#include "AX630C_fastnand_arm64_k419.dtsi"
/ {
	memory@40000000 {
		device_type = "memory";
		reg = <0x0 0x40000000 0x0 0x40000000>;
	};
	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;
#ifdef SUPPORT_RISCV
		riscv_reserved: riscv_memreserved {
			reg = <RISCV_RESERVED_START_HI RISCV_RESERVED_START_LO RISCV_RESERVED_SIZE_HI RISCV_RESERVED_SIZE_LO>;
		};
#endif
#ifdef SUPPORT_RISCV_LOG_MEM
		riscv_log_reserved: riscv_log_memreserved {
			reg = <RISCV_LOG_MEM_RESERVED_START_HI RISCV_LOG_MEM_RESERVED_START_LO RISCV_LOG_MEM_RESERVED_SIZE_HI RISCV_LOG_MEM_RESERVED_SIZE_LO>;
			no-map;
		};
#endif
#ifdef SUPPORT_ISP_IMAGE
		isp_image_reserved: isp_image_memreserved {
			compatible = "axera, isp_image";
			reg = <ISP_IMAGE_RESERVED_START_HI ISP_IMAGE_RESERVED_START_LO ISP_IMAGE_RESERVED_SIZE_HI ISP_IMAGE_RESERVED_SIZE_LO>;
		};
#endif
		atf_reserved: atf_memreserved {
			reg = <ATF_RESERVED_START_HI ATF_RESERVED_START_LO ATF_RESERVED_SIZE_HI ATF_RESERVED_SIZE_LO>;
			no-map;
		};
#ifdef OPTEE_BOOT
		optee_reserved: optee_memserved {
			reg = <OPTEE_RESERVED_START_HI OPTEE_RESERVED_START_LO OPTEE_RESERVED_SIZE_HI OPTEE_RESERVED_SIZE_LO>;
			no-map;
		};
#endif
		axera_ddr_retrain@0 {
			compatible = "axera_ddr_retrain";
			reg = <0x0 0x40000000 0x0 0x1000>;
		};
#ifdef SUPPORT_RAMDISK
		ramdisk_mem@140000000 {
			compatible = "axera, ramdisk";
			reg = <0x0 RAMDISK_START_ADDR 0x0 RAMDISK_SIZE>;
			addr = <0x0 RAMDISK_START_ADDR>;
			size = <RAMDISK_SIZE>;
			no-map;
		};
#endif
#ifdef SUPPORT_MODEL_DATA
		model_data_reserved: model_data_memreserved {
			reg = <MODEL_DATA_RESERVED_START_HI MODEL_DATA_RESERVED_START_LO MODEL_DATA_RESERVED_SIZE_HI MODEL_DATA_RESERVED_SIZE_LO>;
		};
#endif

	};

