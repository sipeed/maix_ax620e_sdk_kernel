&i2c1 {
	#address-cells = <0x1>;
	#size-cells = <0x0>;
	status = "okay";
	es7243l: es7243l@16 {
		compatible = "es7243l_MicArray_0";
		reg = <0x16>;
		#sound-dai-cells = <0>;
	};
	es8156: es8156@9 {
		compatible = "everest,es8156";
		reg = <0x9>;
		#sound-dai-cells = <0>;
	};
};

&i2c_slv0 {
	status = "okay";
	#address-cells = <0x1>;
	#size-cells = <0x0>;
	i2c_slave_eeprom: i2c_slave_eeprom@55 {
		compatible = "axera,slave-24c02";
		reg = <0x40000055>;
		status = "okay";
	};
};

&i2c0 {
	status = "okay";
};

&i2c2 {
	status = "okay";
};

&ax_uart0 {
	status = "okay";
};

&wdt0 {
	status = "okay";
