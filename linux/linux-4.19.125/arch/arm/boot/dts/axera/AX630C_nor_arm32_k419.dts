/dts-v1/;
#include "AX620E.dtsi"
/ {
	memory@40000000 {
		device_type = "memory";
		reg = <0x0 0x40000000 0x0 0x40000000>;
	};
	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;
		ramoops_mem@48000000 {
			compatible = "ramoops";
			reg = <0x0 0x48000000 0x0 0xf0000>;
			record-size = <0x40000>;
			console-size = <0x80000>;
			ftrace-size = <0x20000>;
		};
		axera_memory_dump@0 {
			compatible = "axera_memory_dump";
			reg = <0x0 0x480f0000 0x0 0x3000>;
			dump_ranges = <0x0 0x40000000 0x0 0x80000000>;
			no-map;
		};
	#ifdef AX630C_DDR4_RETRAIN
	asdasd
		axera_ddr_retrain@0 {
			compatible = "axera_ddr_retrain";
			reg = <0x0 0x40000000 0x0 0x1000>;
		};
	#endif
	};

	chosen {
		bootargs = BOOTARGS;
	};

